
class Solution {
    // Function to remove duplicates from sorted linked list.
    Node removeDuplicates(Node head) {
        
        Node i= head;
        Node j= head;
        
        while(j!=null){
            if(j.data==i.data)   j= j.next;
          
            
            else {
                i.next=j;
                i=j;
            }
        }
        i.next= j;
        return head;
    }
}
