class Solution {
    Node divide(Node head) {
        // code here
        Node d1= new Node(-1);
        Node d2= new Node(-1);
        Node t1= d1;
        Node t2= d2;
        Node t= head;
        
        while(t!=null){
            if(t.data%2==0){
                t1.next=t;
                t1=t1.next;
            }
            else{
                t2.next=t;
                t2=t2.next;
            }
            t= t.next;
            
        }
           t1.next=d2.next;
            t2.next=null;
            return d1.next;
    }
} 
